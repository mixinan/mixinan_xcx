.container{
   display: flex;
   flex-direction: column;
}

.header{
   display: flex;
   flex-direction: column;
}

.header image{
   width: 100%;
}

.header text{
   background-color: #c0c0c0;
   text-align: center;
   font-size: 38rpx;
   padding: 10px;
   height: 66rpx;
}


.container>image{
   border: 1px solid red;
   border-radius: 18px;
   width: 100px;
   height: 100px;
   margin: 38px auto;
}

.time{
   align-self: center;
}